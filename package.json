{
  "name": "vanillajs-messenger",
  "version": "1.0.0",
  "description": "VanillaJS messenger with component approach",
  "dependencies": {
    "parcel": "^2.0.0-rc.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "handlebars": "^4.7.7"
  },
  "scripts": {
    "dev": "./node_modules/.bin/parcel index.html",
    "remove-dist-linux": "rm -rf dist",
    "remove-dist-windows": "rmdir /q /s dist",
    "pre-build": "./node_modules/.bin/parcel build index.html",
    "build": "npm run remove-dist-linux && npm run pre-build",
    "build-windows": "npm run remove-dist-windows && npm run pre-build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roolman/vanillajs-messenger.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Roolman/vanillajs-messenger/issues"
  },
  "homepage": "https://github.com/Roolman/vanillajs-messenger#readme"
}
